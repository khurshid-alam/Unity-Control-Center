[Desktop Entry]
_Name=Apply theme
_Comment=Sets the default theme
Icon=gnome-settings-theme
Exec=themus-theme-applier %u
Terminal=false
Type=Application
StartupNotify=false
NoDisplay=true
# we don't want to show this at all in the menus
OnlyShowIn=X-NeverShowThis;
MimeType=application/x-gnome-theme-installed;
X-GNOME-Bugzilla-Bugzilla=GNOME
X-GNOME-Bugzilla-Product=gnome-control-center
X-GNOME-Bugzilla-Component=themes:///
X-GNOME-Bugzilla-Version=@VERSION@
